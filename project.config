[access]
	inheritFrom = All-Projects
[submit]
	action = rebase if necessary
[access "refs/heads/*"]
	create = group OnePlus 3
	forgeAuthor = group OnePlus 3
	forgeCommitter = group OnePlus 3
	push = group OnePlus 3
	label-Code-Review = -2..+2 group OnePlus 3
	label-Verified = -1..+1 group OnePlus 3
	submit = group OnePlus 3
